{
	"name": "preact-jsx-i18n",
	"version": "1.0.0",
	"description": "Localization for Preact using JSX components",
	"source": "src/index.js",
	"main": "dist/preact-jsx-i18n.js",
	"module": "dist/preact-jsx-i18n.esm.js",
	"unpkg": "dist/preact-jsx-i18n.umd.js",
	"scripts": {
		"test": "karmatic",
		"build": "microbundle",
		"prepublishOnly": "npm run build -s"
	},
	"keywords": [
		"jsx",
		"intl",
		"i18n",
		"internationalization",
		"localization",
		"preact",
		"translate",
		"translation"
	],
	"repository": "apyos/preact-jsx-i18n",
	"homepage": "https://github.com/apyos/preact-jsx-i18n",
	"authors": [
		"Philippe Schommers <philippe@schommers.be>"
	],
	"license": "MIT",
	"files": [
		"dist"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,json,css,md}": [
			"prettier --write",
			"git add"
		]
	},
	"devDependencies": {
		"husky": "^4.2.3",
		"karmatic": "^1.4.0",
		"lint-staged": "^10.1.2",
		"microbundle": "^0.11.0",
		"prettier": "^2.0.4",
		"webpack": "^4.42.1"
	},
	"dependencies": {
		"dlv": "^1.1.3",
		"preact": "^10.3.4"
	},
	"peerDependencies": {
		"preact": ">=10"
	}
}
